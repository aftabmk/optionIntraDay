[
  {
    "database": {
      "install": "curl -O https://s3.us-west-2.amazonaws.com/dynamodb-local/dynamodb_local_latest.zip",
      "unzip": "unzip dynamodb_local_latest.zip",
      "initiate": "java -Djava.library.path=./DynamoDBLocal_lib -jar DynamoDBLocal.jar -sharedDb"
    }
  },
  {
    "run cron" : "node cron",
    "fetch": "node getJson"
  },
  {
    "install" : "npm install (if package and package-lock.json available)"
  },
  {
    "clone" :"git clone https://github.com/aftabmk/optionScraper.git"
  },
  {
    "linux dependency" : "sudo apt-get update sudo apt-get install libatk1.0-0 libatk-bridge2.0-0 libcups2 libxkbcommon0 libxcomposite1 libxdamage1 libxfixes3 libxrandr2 libgbm1 libasound2t64"
  },
  {
    "configure" : "aws configure",
    "check" : "aws sts get-caller-identity",
    "permission" : "AmazonDynamoDBFullAccess",
    "create table" : "node createTable",
    "check table" : "aws dynamodb list-tables --region ap-south-1"
  }
]
